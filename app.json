{
  "expo": {
    "name": "Mhameaw-expo",
    "slug": "Mhameaw-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Hmameaw.io"
    },
    "android": {
        "googleServicesFile": "./google-services.json",
        "package": "com.Hmameaw.io",
       
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF"
      } 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
